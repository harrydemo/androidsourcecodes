package com.orange.gpstest;

/**
 * Interface to implements to receive chronometer events
 * @author Alan Sowamber
 */
public interface IChronoEventListener {
	
	/**
	 * Called when the chronometer starts
	 */
	abstract void onChronoStart();
	
	/**
	 * Called when the chronometer stops
	 */
	abstract void onChronoStop();
	
	/**
	 * Called when the chronometer reach the timer value
	 */
	abstract void onChronoReached(int timeElapsed);
	

}
