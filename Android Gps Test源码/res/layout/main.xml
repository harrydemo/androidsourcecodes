</TableRow><TableRow android:id="@+id/TableRow02" android:layout_width="wrap_content" android:layout_height="wrap_content"><TextView android:text="Satellite numbers" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/Satellite_numbers" android:paddingLeft="5dp" android:textSize="10pt"></TextView>
<TextView android:text="" android:id="@+id/Satellite_numbers_value" android:layout_height="wrap_content" android:layout_width="fill_parent" android:paddingRight="5dp" android:textSize="18sp" android:paddingLeft="5dp" android:ellipsize="end"></TextView>
</TableRow>



<TableRow android:id="@+id/TableRow01" android:layout_height="wrap_content" android:layout_width="fill_parent">
<TextView android:text="TTFF" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/TTFF" android:paddingLeft="5dp" android:textSize="10pt"></TextView>
<TextView android:text="" android:id="@+id/TTFF_value" android:layout_height="wrap_content" android:layout_width="fill_parent" android:paddingRight="5dp" android:textSize="18sp" android:paddingLeft="5dp" android:ellipsize="end"></TextView>
</TableRow>

<TableRow android:id="@+id/TableRow01" android:layout_height="wrap_content" android:layout_width="fill_parent">
<TextView android:text="Require satellites" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/SatReq" android:paddingLeft="5dp" android:textSize="10pt"></TextView>
<TextView android:text="" android:id="@+id/SatReq_value" android:layout_height="wrap_content" android:layout_width="fill_parent" android:paddingRight="5dp" android:textSize="18sp" android:ellipsize="end" android:paddingLeft="5dp"></TextView>
</TableRow>

<TableRow android:id="@+id/TableRow01" android:layout_height="wrap_content" android:layout_width="fill_parent">
<TextView android:text="Require network" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/NetworkReq" android:paddingLeft="5dp" android:textSize="10pt"></TextView>
