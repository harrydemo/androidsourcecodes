</TableRow><TableRow android:id="@+id/TableRow01" android:layout_height="wrap_content" android:layout_width="fill_parent"><TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/Location_availability" android:paddingLeft="5dp" android:text="AGPS availability" android:textSize="10pt"></TextView>
