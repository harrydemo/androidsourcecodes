<TextView android:text="" android:id="@+id/Session_duration_value" android:layout_height="wrap_content" android:layout_width="fill_parent" android:paddingRight="5dp" android:textSize="18sp" android:ellipsize="end" android:paddingLeft="5dp"></TextView>
</TableRow><TableRow android:id="@+id/TableRow01" android:layout_height="wrap_content" android:layout_width="fill_parent"><TextView android:text="Horizontal accuracy" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/Horizontal_accuracy" android:paddingLeft="5dp" android:textSize="10pt"></TextView>
