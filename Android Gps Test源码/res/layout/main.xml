</TableRow><TableRow android:id="@+id/TableRow01" android:layout_height="wrap_content" android:layout_width="fill_parent"><TextView android:text="Longitude" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/Longitude" android:paddingLeft="5dp" android:textSize="10pt"></TextView>
