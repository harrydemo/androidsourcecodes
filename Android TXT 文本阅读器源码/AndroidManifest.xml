<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.xiexh.hcreader"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".HcReader"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
		<activity android:name=".TxtList">
        	<intent-filter>
                <action android:name="com.xiexh.hcreader.TxtList" />
                <category android:name="android.intent.category.DEFUALT" />
            </intent-filter>
        </activity>
        <activity android:name=".TxtView">
        	<intent-filter>
                <action android:name="com.xiexh.hcreader.TxtView" />
                <category android:name="android.intent.category.DEFUALT" />
            </intent-filter>
        </activity>
</application>
    <uses-sdk android:minSdkVersion="3" />



</manifest> 