<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:layout_height="wrap_content"
	android:orientation="vertical">
	<LinearLayout android:id="@+id/LinearLayout01"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:orientation="horizontal">
		<TextView android:text="@string/contact_group" android:id="@+id/contact_group_label"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:layout_marginLeft="5dip" android:layout_marginTop="5dip" />
		<Spinner android:id="@+id/contact_group" android:layout_width="wrap_content"
			android:layout_height="wrap_content" />
	</LinearLayout>
	<LinearLayout android:id="@+id/LinearLayout02"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:orientation="horizontal">
		<TextView android:text="@string/contact_label_name"
			android:id="@+id/contact_name_label" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_marginLeft="5dip"
			android:layout_marginTop="5dip" />
		<EditText android:text="" android:id="@+id/contact_name"
			android:layout_width="200dip" android:layout_height="wrap_content" />
	</LinearLayout>
</LinearLayout>
