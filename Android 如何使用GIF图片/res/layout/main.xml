<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:adsdk="http://schemas.android.com/apk/res/com.giftest.fneg"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
<TextView  
    android:layout_width="fill_parent" 
    android:layout_height="wrap_content" 
    android:text="@string/hello"
    />
    <com.wooboo.adlib_android.WoobooAdView
				adsdk:testing="false" adsdk:backgroundColor="#f68e55"
				adsdk:refreshInterval="20" android:layout_height="wrap_content"
				android:layout_width="fill_parent" adsdk:textColor="#009900"
				android:fitsSystemWindows="true" />
    <com.ant.liao.GifView
    android:id="@+id/gifView"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    ></com.ant.liao.GifView>
</LinearLayout>
