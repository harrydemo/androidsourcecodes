<?xml version="1.0" encoding="utf-8"?>
<manifest package="cn.wongxming.ringer"
      android:versionCode="1"
      android:versionName="1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".RingToggle"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    <receiver android:name="AlarmBroadcastReceiver">
            <intent-filter>
               <action android:name="cn.wongxming.ringer.RV_CHANGED" />
               <action android:name="cn.wongxming.ringer.RING_CHANGED" />
               <action android:name="cn.wongxming.ringer.VIBRATE_CHANGED" />
               <action android:name="cn.wongxming.ringer.SILENT_CHANGED" />
            </intent-filter>
        </receiver>

	</application>
    <uses-sdk android:minSdkVersion="3" />
</manifest> 