<LinearLayout android:id="@+id/LinearLayout01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<RelativeLayout android:id="@+id/RelativeLayout01"
		android:layout_width="wrap_content" android:layout_height="wrap_content">
		<ImageView android:id="@+id/ImageView01" android:src="@drawable/other"
			android:layout_marginTop="6dp" android:layout_width="wrap_content"
			android:layout_height="wrap_content" ></ImageView>
		<TextView  android:id="@+id/TextView01"
			android:textColor="#6495ED"
			android:layout_toRightOf="@id/ImageView01" android:textSize="18dp"
			android:layout_marginTop="5dp" android:layout_width="wrap_content"
			android:layout_height="wrap_content"></TextView>
		<TextView  android:id="@+id/TextView02"
			android:layout_toRightOf="@id/ImageView01" android:layout_below="@id/TextView01"
			android:textSize="12dp" android:layout_width="wrap_content" android:textColor="#6495ED"
			android:layout_height="wrap_content"></TextView>
		</RelativeLayout>
	</LinearLayout>
