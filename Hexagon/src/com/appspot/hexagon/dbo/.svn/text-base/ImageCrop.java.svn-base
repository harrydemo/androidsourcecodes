package com.appspot.hexagon.dbo;

import android.provider.BaseColumns;

/**
 * @author Admin
 *
 */
public class ImageCrop {

	public interface Constants extends BaseColumns {
		// table:hexagon.WorldMap
		public static final String TABLE = "imagecrop";
		// columns
		public static final String _ID = "img_id";
		public static final String NAME = "img_name";
		public static final String DESC = "img_desc";
		public static final String PARENT = "img_parent_gal";
		public static final String ROW = "img_row";
		public static final String COLUMN = "img_column";
		public static final String WIDTH = "img_width";
		public static final String HEIGHT = "img_height";
	}
	
	public ImageCrop(){
		
	}
	
	public ImageCrop(int id, String name, String desc, int parent, int row,
			int column, int width, int height) {
		super();
		this.id = id;
		this.name = name;
		this.desc = desc;
		this.parent = parent;
		this.row = row;
		this.column = column;
		this.width = width;
		this.height = height;
	}
	
	private int id = -1;
	private String name = null;
	private String desc = null;
	private int parent = -1;
	private int row = -1;
	private int column = -1;
	private int width = 0;
	private int height = 0;
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getDesc() {
		return desc;
	}
	public void setDesc(String desc) {
		this.desc = desc;
	}
	public int getParent() {
		return parent;
	}
	public void setParent(int parent) {
		this.parent = parent;
	}
	public int getRow() {
		return row;
	}
	public void setRow(int row) {
		this.row = row;
	}
	public int getColumn() {
		return column;
	}
	public void setColumn(int column) {
		this.column = column;
	}
	public int getWidth() {
		return width;
	}
	public void setWidth(int width) {
		this.width = width;
	}
	public int getHeight() {
		return height;
	}
	public void setHeight(int height) {
		this.height = height;
	}	
	
	
}
