    android:id="@+id/TextView01" 
    android:layout_width="fill_parent"  
    android:layout_height="wrap_content" 
    android:text="@string/hello" 
    android:layout_x="100px" 
    android:layout_y="360px" 
    android:textColor="@android:color/black"
  />  
</AbsoluteLayout>
